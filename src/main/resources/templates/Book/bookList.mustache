{{>layout/header}}
{{>layout/nav}}
<div class="container">
{{#master}}
<a href="/books/bookSave" role="button" class="btn btn-outline-dark float-right">책등록</a>
{{/master}}
<h1 class="d-flex justify-content-center">책 목록</h1>
<div class="col-md-12 d-flex justify-content-center">
<table class="list" >
    <tbody>
        {{#bookList}}
        <tr style="border-bottom: 1px solid #444444">
            <td>
                <p>책제목 : <a href="/books/bookInfo/{{isbn}}" class="text-red">{{bookName}}</a> | 장르 : {{bookGen}}</p>
                <p>저자 : {{bookAut}} | 출판사 : {{bookHouse}} | 출판일 : {{bookDate}}</p>
                <p>가격 : {{bookPri}}</p>
                <p class="border-bottom: 1px solid #444444">재고량 : {{bookSto}}</p>
            </td>
        </tr>
        {{/bookList}}
    </tbody>
</table>
</div>
</div>
{{>layout/bookFooter}}